{
    "name": "alikdex/notes",
    "description": "Notes module. Yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","notes"],
    "license": "MIT",
    "authors": [
        {
            "name": "AlikDex",
            "email": "alikdex@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.15"
    },
    "autoload": {
        "psr-4": {
            "Adx\\Module\\NoteModule\\": "./src"
        }
    },
    "post-update-cmd": [
        "php yii migrate --migrationPath=@vendor/alikdex/notes/src/Migration"
    ],
    "post-install-cmd": [
        "php yii migrate --migrationPath=@vendor/alikdex/notes/src/Migration"
    ]
}
